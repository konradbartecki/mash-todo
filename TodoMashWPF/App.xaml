<Application x:Class="TodoMashWPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TodoMashWPF"
             xmlns:model="clr-namespace:TodoMashWPF.Model"
             xmlns:viewmodels="clr-namespace:MashTodoWPF.Core.ViewModels"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <model:StatusToIsCompletedConverter x:Key="StatusToIsCompletedConverter"/>
        <model:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
        <model:InversedBoolToVisiblityConverter x:Key="InversedBoolToVisiblityConverter"/>
        <viewmodels:ViewModelLocator x:Key="ViewModelLocator"/>
    </Application.Resources>
</Application>
